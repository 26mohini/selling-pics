
pragma solidity ^0.5.0;

contract sellpics{
    uint picID; // number of pic customer wants buy//
    uint picPRICE;// price of selected picture//

   
  function addpic(uint _picID)public{
      picID=_picID;
  }
  function addprice(uint _picPRICE)public{
    picPRICE=_picPRICE;  
  }
  function getpicprice(uint _picID,uint _picPRICE)public view returns(uint,uint){ // for getting selected pic no and price of that picture//
       
  
      return(_picID,_picPRICE);

  }
}
